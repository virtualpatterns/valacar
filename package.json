{
  "name": "valacar",
  "version": "0.0.1",
  "description": "Import DHCP leases into a database",
  "bin": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/valacar"
  },
  "scripts": {
    "_install": "node index.js install --enableTrace",
    "_uninstall": "node index.js uninstall --enableTrace",
    "_import": "scp PIGWIDGEON.local:/var/lib/dhcp/dhcpd.leases ./process/data; node index.js import ./process/data/dhcpd.leases --enableTrace",
    "_clean": "node index.js clean --enableTrace",
    "select-tmigration": "sqlite3 -header -csv -nullvalue NULL ./process/data/valacar.db < resources/select-tmigration.sql",
    "select-tversion": "sqlite3 -header -csv -nullvalue NULL ./process/data/valacar.db < resources/select-tversion.sql",
    "select-tlease-active": "sqlite3 -header -csv -nullvalue NULL ./process/data/valacar.db < resources/select-tlease-active.sql",
    "test": "./node_modules/.bin/mocha --require tests/library/setup.js tests"
  },
  "author": "code@virtualpatterns.com",
  "license": "ISC",
  "dependencies": {
    "app-module-path": "^1.0.6",
    "async": "^2.0.0-rc.3",
    "commander": "^2.9.0",
    "dhcpd-leases": "0.0.1",
    "sqlite3": "^3.1.3",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "mocha": "^2.4.5"
  }
}
