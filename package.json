{
  "name": "valacar",
  "version": "0.0.1",
  "description": "Import DHCP leases into a database",
  "bin": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/valacar"
  },
  "scripts": {
    "setup": "node index.js install",
    "teardown": "node index.js uninstall",
    "import": "scp PIGWIDGEON.local:/var/lib/dhcp/dhcpd.leases ./process/data && node index.js import ./process/data/dhcpd.leases",
    "clean": "node index.js clean",
    "dumpLeases": "node index.js dumpLeases",
    "dumpLeasesWhere": "node index.js dumpLeasesWhere",
    "dumpTranslations": "node index.js dumpTranslations",
    "select-tmigration": "sqlite3 -header -csv -nullvalue NULL ./process/data/valacar.db < resources/select-tmigration.sql",
    "select-tversion": "sqlite3 -header -csv -nullvalue NULL ./process/data/valacar.db < resources/select-tversion.sql",
    "test": "mocha --require test.js"
  },
  "author": "code@virtualpatterns.com",
  "license": "ISC",
  "dependencies": {
    "app-module-path": "^1.0.6",
    "async": "^2.0.0-rc.3",
    "cli-table": "^0.3.1",
    "commander": "^2.9.0",
    "dhcpd-leases": "0.0.1",
    "sqlite3": "^3.1.3",
    "winston": "^2.2.0"
  }
}
